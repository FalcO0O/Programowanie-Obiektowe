<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            minHeight="550.0" minWidth="850.0"
            fx:controller="agh.ics.oop.presenter.SimulationPresenter">
    <center>
        <VBox>
            <TextField fx:id="movesListTextField"> </TextField>
            <Label fx:id="moveInfoLabel"/>
            <GridPane fx:id="mapGrid" gridLinesVisible="true"/>
            <Button onAction="#onSimulationStartClicked" >Start</Button>
        </VBox>
    </center>
</BorderPane>
